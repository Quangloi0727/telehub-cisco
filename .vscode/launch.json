    /**
    hiển thị log ở vs code khi debug
    https://stackoverflow.com/questions/48027266/winston-logger-nodejs-debug-console-logs-not-showing-in-vscode
    */
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outputCapture": "std",
            "env": { "NODE_ENV": "development" },
            "program": "${workspaceFolder}\\auth_server.js",
        }
        
    ]
}